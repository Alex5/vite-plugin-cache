{
  "name": "vite-plugin-cache",
  "version": "1.2.6",
  "author": "alex5",
  "license": "MIT",
  "repository": "git+https://github.com/Alex5/vite-plugin-cache.git",
  "keywords": [
    "vite",
    "plugin",
    "workbox",
    "pwa",
    "service-worker",
    "cache"
  ],
  "description": "Zero-config Vite plugin to add Workbox-based service worker with stale-while-revalidate caching.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./sw": "./dist/sw.js"
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "dist/sw.js"
  ],
  "scripts": {
    "build": "rslib build",
    "postbuild": "esbuild src/sw.ts --bundle --format=esm --target=es2020 --outfile=dist/sw.js --minify",
    "check": "biome check --write",
    "dev": "rslib build --watch",
    "format": "biome format --write"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@rslib/core": "^0.9.2",
    "@types/node": "^22.15.31",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "vite": "^6.3.5"
  },
  "peerDependencies": {
    "esbuild": "*"
  }
}
